	<DataTemplate x:Key="ButtonPromptX">
		<Viewbox>
			<Image RenderOptions.BitmapScalingMode="Fant" Source="{pmrk:ThemeFile 'Images/Buttons/Generic/ButtonPromptX.png'}" />
		</Viewbox>
	</DataTemplate>

	<DataTemplate x:Key="ButtonPromptY">
		<Viewbox>
			<Image RenderOptions.BitmapScalingMode="Fant" Source="{pmrk:ThemeFile 'Images/Buttons/Generic/ButtonPromptY.png'}" />
		</Viewbox>
	</DataTemplate>
</ResourceDictionary>
