<ResourceDictionary
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:pmrk="clr-namespace:Playnite.FullscreenApp.Markup;assembly=Playnite.FullscreenApp">
	<DataTemplate x:Key="ButtonPromptA">
		<Viewbox>
			<Image RenderOptions.BitmapScalingMode="Fant" Source="{pmrk:ThemeFile 'Images/Buttons/Generic/ButtonPromptA.png'}" />
		</Viewbox>
	</DataTemplate>

	<DataTemplate x:Key="ButtonPromptB">
